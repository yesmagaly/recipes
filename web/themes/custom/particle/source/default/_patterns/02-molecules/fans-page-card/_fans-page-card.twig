<div class="fans-page-card">
  <a href="#" class="fans-page-card__img">
    <img src="{{ fans_page_card__img.src }}" alt="{{ fans_page_card__img.alt }}">
  </a>
  <div class="fans-page-card__content">
    <a href="#">
      <i class="fab fa-facebook-f"></i>
    </a>
    
    {% block tag %}
      {% include '@atoms/_tag.twig' with { tag__label: fans_page_card__tag } %}
    {% endblock %}
  </div>
</div>
